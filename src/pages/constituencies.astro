---
import counties from '../data/counties';
let constituencies_count=0;
let const_index=0;
counties.forEach(item => {
    constituencies_count+=item.constituencies.length
})

let title = `List of ${constituencies_count} constituencies in Kenya`;
---
<html lang="en">
<head>
    <!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-808D5DTLQJ"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-808D5DTLQJ');
</script>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{title}</title>
    <link rel="icon" type="image/svg+xml" href="/favicon.svg">
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">
<meta name="title" content={title}>
<meta name="description" content={title}>
<meta name="keywords" content="constituencies, Kenya, ">
<meta name="robots" content="index, follow">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<meta name="language" content="English">


    <link
        rel="stylesheet"
        href={Astro.resolve("../assets/style/global.css")}
    >
</head>
<body>

    <div class=" bg-white min-h-screen overflow-auto">
        <nav class="nav border-b flex justify-between container  items-center mx-auto px-4 py-3">
        <div class="flex items-center uppercase font-bold">
            <img src="data:image/png;base64,"
             class=" h-6 w-12  lg:h-10 lg:w-20 object-contain"
             
            alt="logo">
            KENYA
        </div>

        <!-- MOBILE MENU -->

            <div class="lg:hidden">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" 
                stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M3.75 6.75h16.5M3.75 12h16.5m-16.5 5.25h16.5" />
                </svg>
                
            </div>
        <!-- END MOBILE MENU -->
        <div class="hidden lg:flex  items-center text-red-500 gap-4">
            <a href="/"  class="hover:text-black">Counties</a>
            <a href="/constituencies" class="hover:text-black">Contituencies</a>
            <a href="/subcounties"  class="hover:text-black">Sub Counties</a>
            <a href=""  class="hover:text-black">Wards</a>
        </div>
    </nav>
        <div class="container max-w-5xl mx-auto px-4 py-4">
   <h1 class="text-lg lg:text-2xl mb-3 text-gray-600 mb-4">{title}</h1>
   <table class="table w-full border border-collapse">
     <thead>
        <tr class="border-b">
            <td class="px-2 py-1 uppercase font-bold">Code</td>
            <td class="px-2 py-1 uppercase font-bold">Name</td>
            <td class="px-2 py-1 uppercase font-bold">County</td>
        </tr>
     </thead>
<tbody class="">
    {counties.map((county,index)=> {
 
       return  county.constituencies.map(item =>{
        const_index++;
            return (<tr class="bg-white rounded mb-4 px-4 py-2 text-sm lg:text-xl  font-normal text-green-800">
            <td class="px-2 py-1 ">{const_index }</td>
            <td class="px-2 py-1 ">{item}</td>
            <td class="px-2 py-1 ">{county.name}</td>

            
            </tr>)
        })

      })}
</tbody>
   </table>
        </div>
    </div>
</body>
</html>
